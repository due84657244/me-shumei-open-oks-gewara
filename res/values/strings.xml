<?xml version="1.0" encoding="utf-8"?>
<resources>
    
    <string name="app_name">《一键签到》格瓦拉生活网红包签到插件</string>
    <string name="label_activity_main">格瓦拉生活网红包签到插件</string>
    
    
    
    <!-- “添加任务”的弹出窗口上显示的插件名称 -->
    <string name="cfg_plugin_name">格瓦拉生活网红包</string>
    
    <!-- 插件作者 -->
    <string name="cfg_author">家前李树</string>
    
    <!-- “添加任务”的弹出窗口上显示的说明信息，提示用户怎么使用此插件 -->
    <string name="cfg_info">适用于格瓦拉生活网每日红包领取，仅支持邮箱或手机号码登录，不支持合作网站账号登录。\n支持稳赚型(稳赚2积分)、冒险型(随机领取-5-5积分，有几率领取100积分)红包，不支持“微博控”红包。\n</string>
    
    <!--
    在“添加任务”的弹出窗口上显示的“配置”界面类型
    0表示不需要显示显示任何配置界面
    1表示直接由主程序提供一个简单的文本框来让用户输入数据
    2表示由插件自己来完成配置界面的高级定制
    -->
    <string name="cfg_config_type">2</string>
    
    <!-- 该任务每日可签到的次数 -->
    <string name="cfg_times">1</string>

</resources>